{
    "name": "miguilim/filament-auto-resource",
    "description": "A plugin that allows your Resource table and form fields to be constructed at execution time.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Miguilim\\FilamentAutoResource\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "filament/filament": "^3.0",
        "doctrine/dbal": "^3.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Miguilim\\FilamentAutoResource\\FilamentAutoResourceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
